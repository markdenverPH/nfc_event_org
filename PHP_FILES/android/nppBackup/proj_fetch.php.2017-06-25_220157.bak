<?php
	$host = "127.0.0.1";
	$user = "root";
	$pass="";
	$dbname="dborder";
	$con = mysqli_connect($host, $user, $pass, $dbname);
	$sql = "SELECT nfc_id FROM users";
	$result = mysqli_query($con, $sql);
	$array['items'] = array();
	while($row = mysqli_fetch_array($result)){
		array_push($array['items'], array('nfc_id'=>$row[0]));
	}
	echo json_encode($array);
?>